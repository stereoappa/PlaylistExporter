{
   "parser" : "babel-eslint",
   "env": {
       "es6": true,
       "browser": true,
       "node": true
   },
   "rules":{
       "semi": "off",
       "arrow-parens":"off",
       "linebreak-style":"off",
       "comma-dangle":"off",
       "require-jsdoc":"off",
       "no-debugger":"off",
       "no-invalid-this":"off",
       "max-len":"off"
   },
   "extends":["eslint:recommended", "google"]
}